<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" 
		   width="75" height="75" xmlns:utils="net.undf.abicloud.utils.*"
		   verticalScrollPolicy="off" horizontalScrollPolicy="off">
	
	
	<mx:Script>
		<![CDATA[
			import net.undf.abicloud.business.managers.VirtualImageManager;
		]]>
	</mx:Script>
	
	<utils:ImageDefault id="iconImage" horizontalCenter="0" verticalCenter="0"
			  		    width="90%" height="90%" scaleContent="true" source="{data.path}" toolTip="{data.name}"
			  		    defaultImagePath="{VirtualImageManager.DEFAULT_ICON_IMAGE_PATH}"/>
</mx:Canvas>
