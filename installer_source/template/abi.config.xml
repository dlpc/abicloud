<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>
        Properties files containing configuration properties for abiserver
    </comment>
    
    <!--AbiCloud destination -->
    <entry key="destination">http://localhost:##tomcatPort##/abicloud_WS/</entry>


    <!-- Timeout in milliseconds-->
    <entry key="timeout">30000</entry>


    <!--Datacenter name-->
    <entry key="dataCenterName">myDatacenter</entry>

    <entry key="db.url">jdbc:mysql://##databaseHost##:##databasePort##/##databaseName##</entry>
    <entry key="db.username">##databaseUsername##</entry>
    <entry key="db.password">##databasePassword##</entry>
    
    
    <!--  Session timeout in minutes -->
    <entry key="sessionTimeout">60</entry>
    
    <!-- Maximum number of session. A value of 0 means no limit -->
    <entry key="">1</entry>
    
</properties>
