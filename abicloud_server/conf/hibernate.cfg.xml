    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/VirtualimageTypeHB.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/Dns.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/Session.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/AuthGroup.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/AuthServerresource.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/AuthServerresourceException.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/AuthClientresource.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/AuthClientresourceException.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/Enterprise.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/VirtualDataCenter.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/Log.hbm.xml"/>
    <mapping resource="com/abiquo/abiserver/business/hibernate/hbm/Datacenters_IP.hbm.xml"/>
